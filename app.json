{
  "expo": {
    "name": "TransitApp",
    "slug": "TransitAppDebug",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3d404c"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#3d404cff"
      },
      "package": "com.ddev11.TransitAppDebug"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@maplibre/maplibre-react-native",
      [
        "expo-asset",
        {
          "assets": [
            "assets"
          ]
        }
      ]
    ],
    "assetBundlePatterns": [
      "assets/**"
    ],
    "packagerOpts": {
      "assetExts": [
        "html",
        "css",
        "js",
        "txt"
      ]
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8163a953-632b-4f95-b2c2-03cc383a7c7c"
      }
    }
  }
}